<%= form_for [@user, @post], html: { multipart: true  } do |f|  %>

	

	<%= f.label "Title"   %>
	<%= f.text_field :title, class: 'form-control'   %>
	<br>
	<%= f.label "image"   %>
	<%= f.file_field :image   %>
	<br>
	<br>
	<%= f.label "Content"   %>
	<%= f.cktext_area :content, class: 'form-control'   %>
	<br>
	<%= f.text_field :tag_list   %>
	<br>
	<%= f.button :submit %>
	<br>
	<br>
	<%= link_to 'Back', posts_path, class: 'btn btn-primary' %>
<% end %>

